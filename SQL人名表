select *
from ((((((( [社員]
left join [部署]
on [社員].[社員番号] = [部署].[社員番号])
left join [役職]
on [社員].[社員番号] = [役職].[社員番号])
left join [職位]
on [社員].[社員番号] = [職位].[社員番号])
left join [人員区分]
on [社員].[社員番号] = [人員区分].[社員番号])
left join [会社]
on [社員].[社員番号] = [会社].[社員番号])
left join [原価センタ]
on [社員].[社員番号] = [原価センタ].[社員番号])
left join [考課]
on [社員].[社員番号] = [考課].[社員番号]);
